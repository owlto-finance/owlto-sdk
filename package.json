{
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "owlto-sdk",
  "author": "gemeniteg <gemeniteg@gmail.com>",
  "module": "dist/owlto-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/owlto-sdk.cjs.production.min.js",
      "limit": "1000 KB"
    },
    {
      "path": "dist/owlto-sdk.esm.js",
      "limit": "1000 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.2",
    "husky": "^9.0.11",
    "size-limit": "^11.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
